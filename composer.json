{
    "name": "crowdstar/template",
    "description": "A PHP template engine used at Glu Mobile.",
    "homepage": "https://www.glu.com",
    "license": "Apache-2.0",
    "type": "library",
    "keywords": [
        "PHP",
        "templates",
        "template-engine"
    ],
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "mustache/mustache": "~2.11"
    },
    "suggest": {
        "mustache/mustache": "Use the Mustache template engine to render templates."
    },
    "autoload": {
        "psr-4": {
            "CrowdStar\\Template\\": "src/"
        }
    }
}
